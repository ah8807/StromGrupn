@model IEnumerable<web.Models.Izvedba>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DatumUra)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NadomestniUcitelj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skupina)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DatumUra)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NadomestniUcitelj.UciteljID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Skupina.SkupinaID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IzvedbaID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IzvedbaID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IzvedbaID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
